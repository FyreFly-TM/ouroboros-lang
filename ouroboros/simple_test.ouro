// Simple test for new Ouroboros language features

import "urania";

// Main function
function main() {
    print("=== Testing New Language Features ===");
    
    // Test typed variables
    let int_var = 42;
    let float_var = 3.14;
    let string_var = "Hello";
    
    print("Basic variable values:");
    print("int_var: " + to_string(int_var));
    print("float_var: " + to_string(float_var));
    print("string_var: " + string_var);
    
    // Test simple function
    let result = add(10, 20);
    print("10 + 20 = " + to_string(result));
    
    print("Simple test for Vector class:");
    
    // Create a new vector
    var vec = new Vector(1.0, 2.0, 3.0);
    
    // Access vector properties
    print("Vector properties: " + vec.x + ", " + vec.y + ", " + vec.z);
    
    // Call vector method
    print("Vector string: " + vec.to_string());
    
    print("=== All tests completed successfully! ===");
    
    return 0;
}

// Simple function for testing
function add(a, b) {
    return a + b;
} 